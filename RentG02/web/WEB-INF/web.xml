<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Conexion</servlet-name>
        <servlet-class>packBD.Conexion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SrvLoguearse</servlet-name>
        <servlet-class>packServlets.SrvLoguearse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GestorClientes</servlet-name>
        <servlet-class>packBD.GestorClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cliente</servlet-name>
        <servlet-class>packModelo.Cliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvrRegistrarse</servlet-name>
        <servlet-class>packServlets.SvrRegistrarse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GestorRegistrarse</servlet-name>
        <servlet-class>packBD.GestorRegistrarse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GestorRS</servlet-name>
        <servlet-class>packBD.GestorRS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvrLogoff</servlet-name>
        <servlet-class>packServlets.SvrLogoff</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Conexion</servlet-name>
        <url-pattern>/Conexion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SrvLoguearse</servlet-name>
        <url-pattern>/SrvLoguearse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GestorClientes</servlet-name>
        <url-pattern>/GestorClientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cliente</servlet-name>
        <url-pattern>/Cliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvrRegistrarse</servlet-name>
        <url-pattern>/SvrRegistrarse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GestorRegistrarse</servlet-name>
        <url-pattern>/GestorRegistrarse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GestorRS</servlet-name>
        <url-pattern>/GestorRS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvrLogoff</servlet-name>
        <url-pattern>/SvrLogoff</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
